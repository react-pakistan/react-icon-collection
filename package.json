{
  "name": "@taimoormk/react-icon-collection",
  "version": "1.0.8",
  "description": "React Icons Collection built upon React StoryBook",
  "module": "lib/index.js",
  "main": "lib/index.js",
  "types": "index.d.tsx",
  "scripts": {
    "build": "yarn clean && yarn build:ts && cp package.json lib && cp README.md lib",
    "build:storybook": "build-storybook -c .storybook -o .out",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:ts:cjs": "tsc --project tsconfig.build-cjs.json",
    "clean": "yarn rimraf ./lib",
    "deploy:storybook": "storybook-to-ghpages",
    "generate:icons": "yarn generate:script && scripts/generate-icons.sh",
    "generate:script": "scripts/generate-script.sh",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:staged": "lint-staged",
    "lint:css": "stylelint './src/**/*.tsx'",
    "lint:ts": "eslint --config eslint.config.js",
    "publish:github": "cd lib/ && npm publish && cd ..",
    "publish:npm": "cd lib/ && npm publish --access public && cd ..",
    "start": "start-storybook -p 9001 -c .storybook",
    "test": "jest --coverage --config jest/config.json --passWithNoTests",
    "upgrade": "ncu -u"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Taimoormk/react-icon-collection.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "keywords": [
    "4thofjuly",
    "block chain",
    "brands",
    "business",
    "food dishes",
    "green energy",
    "helmet",
    "home",
    "kitchen",
    "knife",
    "law",
    "leaf",
    "logistics",
    "lumberjack",
    "medical",
    "mobile",
    "music",
    "office",
    "security",
    "shopping",
    "skull",
    "space",
    "spring",
    "sweet 17",
    "time",
    "travel",
    "valentine",
    "virtual reality",
    "zoo"
  ],
  "author": "Taimoor Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Taimoormk/react-icon-collection/issues"
  },
  "homepage": "https://github.com/Taimoormk/react-icon-collection#readme",
